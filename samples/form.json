                    "options": {
                        "type": "http",
                        "request": {
                            "url": "http://guanyin.gvg.com.tr/service/rest/city",
                            "params": {
                                "countryOid": "${country}"
                            }
                        }
                    },
                    "titleKey": "name",
                    "valueKey": "oid",
                    "onChange": [
