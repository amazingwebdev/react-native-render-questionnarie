                    "title": "Ülke",
                    "type": "list",
                    "tag": "country",
                    "required": true,
                    "photoRequired": true,
                    "options": {
                        "type": "http",
                        "request": {
                            "url": "http://guanyin.gvg.com.tr/service/rest/country"
