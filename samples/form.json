                    "required": true,
                    "options": {
                        "type": "http",
                        "request": {
                            "url": "http://guanyin.gvg.com.tr/service/rest/county/getbycity",
                            "params": {
                                "cityOid": "${city}"
                            }
                        }
                    },
                    "titleKey": "name",
                    "valueKey": "oid"
